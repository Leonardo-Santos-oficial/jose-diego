{
  "name": "aviator-node-service",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --out-dir dist --clean",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.81.1",
    "dotenv": "16.4.5",
    "fastify": "5.6.2",
    "pino": "9.14.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.11.6",
    "eslint": "9.7.0",
    "tsup": "8.3.6",
    "tsc-alias": "1.8.10",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.7.0",
    "tsx": "4.19.0",
    "typescript": "5.4.5",
    "vitest": "2.1.9"
  },
  "overrides": {
    "esbuild": "0.27.0"
  }
}
